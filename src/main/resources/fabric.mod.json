{
  "schemaVersion": 1,
  "id": "flywheel",
  "version": "${version}",

  "name": "Flywheel Unofficial",
  "description": "A modern engine for modded minecraft.",
  "authors": [
    "Jozufozu",
    "Pepper_Bell",
    "TexTrue",
    "KessokuTeaTime"
  ],
  "contact": {
    "homepage": "https://github.com/KessokuTeaTime/flywheel-fabric",
    "issues": "https://github.com/KessokuTeaTime/flywheel-fabric/issues",
    "sources": "https://github.com/KessokuTeaTime/flywheel-fabric"
  },

  "license": "MIT",
  "icon": "logo.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "com.jozufozu.flywheel.Flywheel"
    ]
  },
  "mixins": [
    "flywheel.mixins.json",
    "flywheel.fabric.mixins.json",
    "flywheel.sodium.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": ">=0.84.0",
    "minecraft": ">=1.20.1",
    "java": ">=17"
  }
